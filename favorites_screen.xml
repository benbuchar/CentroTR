<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.ben.myapplication.Favorites"
    android:background="#91454945">

    <!-- TODO: Update blank fragment layout -->

    <Button
        android:layout_width="120dp"
        android:layout_height="100dp"
        android:text="TJ Maxx"
        android:id="@+id/button6"
        android:layout_gravity="right|center_vertical"
        android:background="#ff28946d"
        android:textColor="#FFFFFF"
        android:textSize="15dp" />

    <Button
        android:layout_width="120dp"
        android:layout_height="100dp"
        android:text="Maurice&apos;s"
        android:id="@+id/button7"
        android:layout_gravity="left|bottom"
        android:background="#ff28946d"
        android:textColor="#FFFFFF"
        android:textSize="15dp" />

    <Button
        android:layout_width="120dp"
        android:layout_height="100dp"
        android:text="Bodified"
        android:id="@+id/button8"
        android:layout_gravity="center_horizontal|bottom"
        android:background="#ff28946d"
        android:textColor="#FFFFFF"
        android:textSize="15dp"
        android:singleLine="true" />

    <Button
        android:layout_width="120dp"
        android:layout_height="100dp"
        android:text="Walmart"
        android:id="@+id/button9"
        android:layout_gravity="bottom|center|right"
        android:gravity="center"
        android:background="#ff28946d"
        android:textColor="#FFFFFF"
        android:textSize="15dp" />

    <Button
        android:layout_width="120dp"
        android:layout_height="100dp"
        android:text="Fort Ontario"
        android:id="@+id/button"
        android:layout_gravity="center"
        android:singleLine="true"
        android:background="#ff28946d"
        android:textColor="#FFFFFF"
        android:textSize="15dp" />

    <Button
        android:layout_width="120dp"
        android:layout_height="100dp"
        android:text="Kraftee&apos;s"
        android:id="@+id/button2"
        android:layout_gravity="left|center_vertical"
        android:nestedScrollingEnabled="false"
        android:background="#ff28946d"
        android:textColor="#FFFFFF"
        android:textSize="15dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Small Text"
        android:id="@+id/textView3"
        android:layout_gravity="right|bottom" />

    <TextView
        android:layout_width="240dp"
        android:layout_height="65dp"
        android:text="Favorite Stops"
        android:id="@+id/textView"
        android:layout_gravity="left|top"
        android:textColor="#FFFFFF"
        android:textStyle="normal|bold"
        android:textSize="30dp"
        android:typeface="serif"
        android:singleLine="true"
        android:shadowColor="#000000"
        android:nestedScrollingEnabled="false"
        android:visibility="visible"
        android:background="#ff28946d"
        android:clickable="true"
        android:textAlignment="center" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Home"
        android:id="@+id/button10"
        android:layout_gravity="right|top"
        android:background="#ff4c6394"
        android:onClick="launchHome"/>

</FrameLayout>
